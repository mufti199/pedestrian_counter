#include <stdio.h>
#include <stdlib.h>
#include <math.h>


void test(int array[], int i);

int main(int argc, char *argv[]){
	int *i;
	int array[3];
	test(&array[0], &i);
	printf("%d\n", array[0]);
	printf("%d\n", array[1]);
	printf("%d\n", array[2]);
	printf("%d\n", i);

}

void test(int array[], int i){
	i = 1;
	array[0] = 11;
	array[1] = 12;
	array[2] = 13;
}